{
  "hosting": {
    "public": "daiaoki-portfolio/build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      },
      {
        "source": "/contact", "function": "contact",
        "destination": "/contact"
      }
    ]
  }
}
